{
"CARDS - Tarjetas para las presentaciones": {
  "prefix": "cards:EjerciciosHTML",
  "body": [
    "<article class=\"card__wrap\">",
    "    <h2 class=\"card__title\"><a href=\"$1\" class=\"card__title-link\">$2</a>",
    "    </h2>",
    "    <p class=\"card__description\">$3</p>",
    "    <img src=\"$4\" alt=\"Imagen de la Tarjeta\" class=\"card__image\">",
    "    <footer class=\"card__footer\">",
    "        <p class=\"card__date\">$5</p>",
    "    </footer>",
    "</article>",
    "",
    "",
    "",
    "",
    "<style>",
    ".main {",
    "    display: flex;",
    "    flex-direction: row;",
    "    gap: 2rem;",
    "    justify-content: space-between;",
    "    margin: 1rem;",
    "  }",
    "  .html__cards {",
    "    border-right: 5px dotted var(--color-grey);",
    "    margin-bottom: 1.5rem;",
    "  }",
    "  .js__cards {",
    "    border-left: 5px dotted var(--color-grey);",
    "  }",
    "  .html__cards,",
    "  .js__cards {",
    "    display: flex;",
    "    flex-direction: row;",
    "    flex-wrap: wrap;",
    "    row-gap: 2rem;",
    "    column-gap: 1rem;",
    "    justify-content: space-evenly;",
    "    align-content: flex-start;",
    "    flex-basis: 50%;",
    "  }",
    "  .card__wrap {",
    "    border: 1px solid var(--color-grey);",
    "    border-radius: 15px;",
    "    overflow: hidden;",
    "    width: 350px;",
    "    min-height: 300px;",
    "  ",
    "    box-shadow: 7px 7px 7px gray;",
    "    /* background-color: var(--color-crema); */",
    "    background-color: var(--color-v);",
    "    margin: 2rem;",
    "  }",
    "  .card__title {",
    "    text-align: center;",
    "    font-size: 1.6rem;",
    "    line-height: 3.5rem;",
    "    background-color: var(--color-black-card);",
    "    color: var(--color-grey);",
    "    letter-spacing: 3px;",
    "  }",
    "  .card__title-link {",
    "    text-decoration: none;",
    "    color: var(--color-grey);",
    "  }",
    "  .card__title:hover {",
    "    background-color: var(--color-grey);",
    "  }",
    "  .card__title:hover .card__title-link {",
    "    color: var(--color-semiBlue);",
    "  }",
    "  ",
    "  .card__description {",
    "    font-size: 1.2rem;",
    "    padding: 10px;",
    "  }",
    "  .card__image {",
    "    width: 100%;",
    "    padding: 5px;",
    "    border-radius: 15px;",
    "    margin-bottom: 25px;",
    "  }",
    "  .card__footer {",
    "    position: absolute;",
    "    bottom: 0;",
    "    width: 100%;",
    "  ",
    "    background-color: var(--color-o);",
    "  }",
    "  .card__date {",
    "    text-align: center;",
    "    font-size: 1.2rem;",
    "    font-weight: 600;",
    "  }",
    "</style>"
  ],
  "description": "CARDS - Tarjetas para las presentaciones"
}
}